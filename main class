    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        x =jTextField1.getText();
       
      if(jCheckBox1.isSelected()&&jCheckBox2.isSelected()&&jCheckBox3.isSelected()){
             jTextArea1.setText( x+"  You ares sufffering from Plasmodium parasite");
 
      }
      else if(jCheckBox4.isSelected()&&jCheckBox5.isSelected()&&jCheckBox6.isSelected()){
      jTextArea1.setText( x+"  You are suffering from  some crucial form of \nsicknessHe can try another doctor");
      }
      else if(jCheckBox7.isSelected()){
      jTextArea1.setText (x+"You are  suffering from Over energy syndrome ");
      }
       else if(jCheckBox13.isSelected()&&jCheckBox18.isSelected()||jCheckBox20.isSelected()&&jCheckBox23.isSelected()){
          
       jTextArea1.setText(x+"  You are  suffering from  Thyphoid Fever");
      }
       
        else if(jCheckBox11.isSelected()&&jCheckBox9.isSelected()||jCheckBox15.isSelected()&&jCheckBox17.isSelected()){
          
       jTextArea1.setText(x+"  You are  from excess Jacking\nAdvice this patient to reduce the rate of reading ");
      }
        
       else if(jCheckBox21.isSelected()&&jCheckBox24.isSelected()||jCheckBox22.isSelected()&&jCheckBox23.isSelected()){
          
       jTextArea1.setText(x+"  This Patient is suffering Laziness as a result of lack of vitamin B Complex\n ");
      }
        
      else if(jCheckBox19.isSelected()&&jCheckBox21.isSelected()||jCheckBox15.isSelected()&&jCheckBox17.isSelected()){
          
       jTextArea1.setText(x+"You are from Diabetes\nAdvice this patient to reduce the rate of consuming\nCarbohydrate ");
      }
      
       else if(jCheckBox7.isSelected()&&jCheckBox8.isSelected()||jCheckBox13.isSelected()&&jCheckBox1.isSelected()){
          
       jTextArea1.setText(x+" This symptoms corresponds to Accumulation of fat in the \nsystem due to lack of exercise.\nThe patient should be advised to engage into physical exercise ");
      }
      
      else{ 
      jTextArea1.setText(x+"   Your sickness is very  complicated\n Are you sure of what you said is wrong with you? ");
      }
     
    }//GEN-LAST:event_jButton1ActionPerformed

